package ui;

import java.io.IOException;

import org.openqa.selenium.By;
import org.testng.annotations.Test;

import base.BaseTest;

public class T5 extends BaseTest {
	@Test
	public static void test5() throws IOException, InterruptedException {

		driver.manage().window().maximize();
		String expected = driver.getTitle();
		String actual = "Login | Salesforce";
		softassert.assertEquals(actual, expected);

		driver.findElement(By.xpath(loc.getProperty("username"))).sendKeys("renu@tek.com");
		driver.findElement(By.xpath(loc.getProperty("password"))).sendKeys("Kiaansai2420");
		driver.findElement(By.xpath(loc.getProperty("login_button"))).click();
		System.out.println("Afer successful login the home page titile is: " + driver.getTitle());



}}
