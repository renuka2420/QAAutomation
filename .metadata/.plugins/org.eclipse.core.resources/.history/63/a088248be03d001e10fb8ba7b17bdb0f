package ui;
import java.io.IOException;

import org.openqa.selenium.By;

import base.BaseTest;

public class T1 extends BaseTest{

	public static void main(String[] args) throws IOException, InterruptedException {
		
setUp();
Thread.sleep(5000);
driver.findElement(By.xpath(loc.getProperty("username"))).sendKeys(args);
driver.findElement(By.xpath(loc.getProperty("password"))).sendKeys(args);
driver.findElement(By.xpath(loc.getProperty("login_button"))).click();

tearDown();


		

	}

}
