/*Write a program for binary search. And 5 i/p has to take 


 * from user as binary elements.
 */

package hackathonJava;

import java.util.Arrays;

public class Q25 {
	
	public static void binarySearch(int[] Arr,int Key) 
	{
		Arrays.sort(Arr);
		int res=Arrays.binarySearch(Arr,Key);
		if(res<0) 
		
			
			System.out.println("not foud");
			else
				System.out.println("foud at index  "+res);
		}
		
	

	public static void main(String[] args) {
	int Arr[]= {1,2,3,65,43,32,53,58,67,54,22,};
	int Key=43;
	binarySearch( Arr, Key);
	
	

	}

}
